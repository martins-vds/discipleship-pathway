@page "/.well-known/{*acmeChallenge}"
@inject IJSRuntime JSRuntime

@code{

    [Parameter]
    public string? AcmeChallenge { get; set; }

    public string Challenge => "7OlxOWlhZxrjekdC-0Par93fERK6gt27qAyCg0tfIF0.eqm6LfgvOjo010NB-CBvZRDl1_zXszwTKQCl5r2aT_I";

        protected override async Task OnAfterRenderAsync(bool firstRender)
        {
            byte[] file = System.Text.Encoding.UTF8.GetBytes(Challenge);
            await JSRuntime.InvokeVoidAsync("BlazorDownloadFile", "file.txt", "text/plain", file);
        }
}